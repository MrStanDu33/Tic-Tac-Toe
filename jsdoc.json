{
  "tags": {
    "dictionaries": ["jsdoc"]
  },
  "source": {
    "include": ["./src", "README.md"],
    "includePattern": ".js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "encoding": "utf8",
    "template": "node_modules/better-docs",
    "destination": "docs/",
    "readme": "README.md",
    "recurse": true
  },
  "templates": {
    "cleverLinks": true,
    "monospaceLinks": true,
    "search": true,
    "better-docs": {
      "name": "Tic Tac Toe code documentation",
      "title": "Tic Tac Toe code documentation",
      "hideGenerator": true,
      "navLinks": [
        {
          "label": "Github",
          "href": "https://github.com/SoftwareBrothers/admin-bro"
        },
        {
          "label": "Example Application",
          "href": "https://admin-bro-example-app-staging.herokuapp.com/admin"
        }
      ]
    }
  }
}
